{
  "name": "electron-ts-react-skeleton",
  "version": "1.0.0",
  "description": "electron-ts-react-skeleton",
  "private": "true",
  "main": "main/Launcher.js",
  "scripts": {
    "release": "build --mac"
  },
  "author": "Alex",
  "directories": {
    "buildResources": "release-resources",
    "output": "release",
    "app": "dist"
  },
  "build": {
    "appId": "com.github.electron-ts-react-skeleton",
    "dmg": {
      "window": {
        "width": "550",
        "height": "320"
      },
      "contents": [
        {
          "x": 130,
          "y": 180
        },
        {
          "x": 410,
          "y": 180,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "target": "squirrel"
    },
    "squirrelWindows": {
      "iconUrl": "https://raw.githubusercontent.com/alexlee85/electron-ts-react-skeleton/master/release-resources/icon.ico",
      "msi": false
    }
  },
  "dependencies": {
    "react": "^15.4.1",
    "react-dom": "^15.4.1"
  },
  "devDependencies": {
    "@types/electron": "^1.4.29",
    "@types/es6-promise": "0.0.32",
    "@types/node": "0.0.2",
    "@types/react": "^0.14.54",
    "@types/react-dom": "^0.14.19",
    "css-loader": "^0.26.1",
    "electron": "^1.4.11",
    "electron-builder": "^10.7.0",
    "electron-connect": "^0.6.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "gulp": "^3.9.1",
    "gulp-clean": "^0.3.2",
    "gulp-if": "^2.0.2",
    "gulp-inject": "^4.1.0",
    "gulp-json-editor": "^2.2.1",
    "gulp-rename": "^1.2.2",
    "gulp-typescript": "^3.1.3",
    "gulp-uglify": "^2.0.0",
    "gulp-util": "^3.0.7",
    "html-webpack-plugin": "^2.24.1",
    "minimist": "^1.2.0",
    "node-sass": "^3.13.0",
    "run-sequence": "^1.2.2",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "ts-loader": "^1.3.0",
    "tslint": "^4.0.2",
    "typescript": "^2.1.5",
    "url-loader": "^0.5.7",
    "webpack": "^1.14.0",
    "webpack-stream": "^3.2.0"
  }
}
